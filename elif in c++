#include<iostream>

using namespace std;

int main(){
    int a,b,c;
    

    cout<< "Enter the values of a,b,c"<<endl;
    cin>>a>>b>>c;

    if(a>b && a>c){
        cout<<"the number "<<a<< "is the greatest"<<endl;
    } else if (b >a && b >c){
        cout<<"the number"<<b<< "is the greatest"<<endl;
    } else {
        cout<<"the number"<<c<< "is the greatest"<<endl;
    }
    return 0;
}
